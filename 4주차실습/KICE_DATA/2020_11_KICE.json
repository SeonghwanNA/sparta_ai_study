    ]\
  \},\
  \{\
    "id": "2020_11_KICE_20-23",\
    "paragraph": "\uc0\u54617 \u49373 : \u49440 \u49373 \u45784 , \u50715 \u45216  \u44536 \u47548 \u51012  \u48372 \u47732  \u51328  \u51060 \u49345 \u54644 \u50836 . \u49440 \u49373 \u45784 : \u47924 \u50631 \u51060  \u51060 \u49345 \u54616 \u45796 \u45716  \u44144 \u45768 ? \u54617 \u49373 : \u49324 \u46988 \u46308 \u51060  \u45796  \u46609 \u44057 \u51060  \u49373 \u44220 \u50612 \u50836 . \u50715 \u45216  \u49324 \u46988 \u46308 \u51008  \u45796  \u46609 \u44057 \u51060  \u49373 \u44220 \u45208 \u50836 ? \u49440 \u49373 \u45784 : (\u50883 \u51020 ) \u50500 \u45768 , \u50715 \u45216  \u49324 \u46988 \u46308 \u46020  \u51648 \u44552 \u52376 \u47100  \u45796  \u45796 \u47476 \u44172  \u49373 \u44220 \u45800 \u45796 . \u54617 \u49373 : \u44536 \u47088 \u45936  \u50780  \u44536 \u47548  \u49549  \u49324 \u46988 \u46308 \u51008  \u45796  \u46609 \u44057 \u50500  \u48372 \u51060 \u51424 ? \u49440 \u49373 \u45784 : \u50715 \u45216  \u44536 \u47548 \u51008  \u49324 \u51652 \u52376 \u47100  \u51080 \u45716  \u44536 \u45824 \u47196  \u44536 \u47532 \u45716  \u44163 \u51060  \u50500 \u45768 \u46972 , \u44536 \u47548 \u51012  \u53685 \u54644  \u47924 \u50616 \u44032 \u47484  \u54364 \u54788 \u54616 \u44256 \u51088  \u54664 \u44592  \u46412 \u47928 \u51060 \u50556 . \u54617 \u49373 : \u47924 \u50616 \u44032 \u47484  \u54364 \u54788 \u54616 \u44256 \u51088  \u54664 \u45796 \u45768 \u50836 ? \u49440 \u49373 \u45784 : \u50715 \u45216  \u44536 \u47548 \u50640 \u45716  \u49324 \u46988 \u46308 \u51032  \u49373 \u44033 \u51060 \u45208  \u44032 \u52824 \u44288 , \u45000  \u46321 \u51060  \u45812 \u44200  \u51080 \u45800 \u45796 . \u54617 \u49373 : \u50500 , \u44536 \u47111 \u44396 \u45208 . \u44536 \u47088 \u45936  \u50715 \u45216  \u44536 \u47548 \u51012  \u48372 \u47732 , \u49324 \u46988 \u46308 \u51060  \u45796  \u48708 \u49847 \u54616 \u44172  \u49373 \u44220 \u51012  \u49104 \u47564  \u50500 \u45768 \u46972  \u53356 \u44592 \u46020  \u45796  \u45804 \u46972 \u50836 . \u49440 \u49373 \u45784 : \u50715 \u45216  \u44536 \u47548 \u50640 \u49436 \u45716  \u49324 \u46988 \u51032  \u53356 \u44592 \u47484  \u45796 \u47476 \u44172  \u54364 \u54788 \u54616 \u50668  \u49324 \u54924 \u51201  \u51648 \u50948 \u45208  \u51473 \u50836 \u46020 \u47484  \u45208 \u53440 \u45256 \u45800 \u45796 . \u54617 \u49373 : \u44536 \u47100  \u50715 \u45216  \u44536 \u47548 \u51012  \u48380  \u46412 \u45716  \u49324 \u46988 \u46308 \u51032  \u53356 \u44592 \u47484  \u45576 \u50668 \u44200 \u48400 \u50556 \u44192 \u45348 \u50836 . \u49440 \u49373 \u45784 : \u44536 \u47000 . \u49324 \u46988 \u46308 \u51032  \u53356 \u44592 \u49104 \u47564  \u50500 \u45768 \u46972  \u54364 \u51221 \u51060 \u45208  \u50743 \u52264 \u47548 , \u51452 \u48320  \u49324 \u47932  \u46321 \u51012  \u51088 \u49464 \u55176  \u49332 \u54196 \u48372 \u47732  \u44536 \u47548 \u50640  \u45812 \u44596  \u51032 \u48120 \u47484  \u45908  \u51096  \u51060 \u54644 \u54624  \u49688  \u51080 \u45800 \u45796 .\
    "type": 1,\
    "problems": [\
      \{\
        "question": "\uc0\u50948  \u45824 \u54868 \u50640 \u49436  \u49440 \u49373 \u45784 \u51060  \u49324 \u50857 \u54620  \u49444 \u47749  \u48169 \u48277 \u51004 \u47196  \u44032 \u51109  \u51201 \u51208 \u54620  \u44163 \u51008 ?",\
        "choices": \{\
          "1": "\uc0\u54617 \u49373 \u51032  \u51656 \u47928 \u50640  \u45824 \u54644  \u45813 \u48320 \u51012  \u54924 \u54588 \u54616 \u44256  \u51080 \u45796 .",\
          "2": "\uc0\u54617 \u49373 \u51032  \u51656 \u47928 \u51032  \u51032 \u46020 \u47484  \u50780 \u44257 \u54616 \u50668  \u45813 \u48320 \u54616 \u44256  \u51080 \u45796 .",\
          "3": "\uc0\u54617 \u49373 \u51032  \u51656 \u47928 \u50640  \u45824 \u54644  \u52628 \u49345 \u51201 \u51004 \u47196  \u45813 \u48320 \u54616 \u44256  \u51080 \u45796 .",\
          "4": "\uc0\u54617 \u49373 \u51032  \u51656 \u47928 \u50640  \u45824 \u54644  \u44396 \u52404 \u51201 \u51064  \u50696 \u47484  \u46308 \u50612  \u45813 \u48320 \u54616 \u44256  \u51080 \u45796 .",\
          "5": "\uc0\u54617 \u49373 \u51032  \u51656 \u47928 \u50640  \u45824 \u54644  \u48708 \u50976 \u47484  \u49324 \u50857 \u54616 \u50668  \u45813 \u48320 \u54616 \u44256  \u51080 \u45796 ."\
        \},\
        "answer": "4",\
        "score": 2\
      \},\
      \{\
        "question": "\uc0\u50948  \u45824 \u54868 \u47484  \u48148 \u53461 \u51004 \u47196  \u50715 \u45216  \u44536 \u47548 \u51032  \u53945 \u51669 \u51012  \u49444 \u47749 \u54620  \u44163 \u51004 \u47196  \u51201 \u51208 \u54616 \u51648  \u50506 \u51008  \u44163 \u51008 ?",\
        "choices": \{\
          "1": "\uc0\u50715 \u45216  \u44536 \u47548 \u51008  \u49324 \u46988 \u46308 \u51032  \u49373 \u44033 \u51060 \u45208  \u44032 \u52824 \u44288 , \u45000  \u46321 \u51012  \u45812 \u44256  \u51080 \u45796 .",\
          "2": "\uc0\u50715 \u45216  \u44536 \u47548 \u51008  \u49324 \u46988 \u51032  \u53356 \u44592 \u47484  \u45796 \u47476 \u44172  \u54364 \u54788 \u54616 \u50668  \u49324 \u54924 \u51201  \u51648 \u50948 \u45208  \u51473 \u50836 \u46020 \u47484  \u45208 \u53440 \u45240 \u45796 .",\
          "3": "\uc0\u50715 \u45216  \u44536 \u47548 \u51008  \u49324 \u51652 \u52376 \u47100  \u51080 \u45716  \u44536 \u45824 \u47196  \u44536 \u47532 \u45716  \u44163 \u51060  \u50500 \u45768 \u45796 .",\
          "4": "\uc0\u50715 \u45216  \u44536 \u47548 \u51008  \u49324 \u46988 \u46308 \u51060  \u45796  \u46609 \u44057 \u51060  \u49373 \u44220 \u45796 .",\
          "5": "\uc0\u50715 \u45216  \u44536 \u47548 \u51012  \u48380  \u46412 \u45716  \u49324 \u46988 \u46308 \u51032  \u53356 \u44592 \u49104 \u47564  \u50500 \u45768 \u46972  \u54364 \u51221 \u51060 \u45208  \u50743 \u52264 \u47548 , \u51452 \u48320  \u49324 \u47932  \u46321 \u51012  \u51088 \u49464 \u55176  \u49332 \u54196 \u48372 \u50500 \u50556  \u54620 \u45796 ."\
        \},\
        "answer": "4",\
        "score": 2\
      \},\
      \{\
        "question": "\uc0\u50948  \u45824 \u54868 \u51032  \u45236 \u50857 \u44284  \u51068 \u52824 \u54616 \u51648  \u50506 \u45716  \u44163 \u51008 ?",\
        "choices": \{\
          "1": "\uc0\u54617 \u49373 \u51008  \u50715 \u45216  \u44536 \u47548 \u51012  \u48372 \u44256  \u49324 \u46988 \u46308 \u51060  \u45796  \u46609 \u44057 \u51060  \u49373 \u44220 \u45796 \u44256  \u49373 \u44033 \u54620 \u45796 .",\
          "2": "\uc0\u49440 \u49373 \u45784 \u51008  \u50715 \u45216  \u44536 \u47548 \u51060  \u49324 \u51652 \u52376 \u47100  \u51080 \u45716  \u44536 \u45824 \u47196  \u44536 \u47532 \u45716  \u44163 \u51060  \u50500 \u45768 \u46972 \u44256  \u49444 \u47749 \u54620 \u45796 .",\
          "3": "\uc0\u54617 \u49373 \u51008  \u50715 \u45216  \u44536 \u47548 \u50640 \u49436  \u49324 \u46988 \u46308 \u51032  \u53356 \u44592 \u44032  \u45796 \u47480  \u51060 \u50976 \u47484  \u47931 \u45716 \u45796 .",\
          "4": "\uc0\u49440 \u49373 \u45784 \u51008  \u50715 \u45216  \u44536 \u47548 \u50640 \u49436  \u49324 \u46988 \u51032  \u53356 \u44592 \u44032  \u49324 \u54924 \u51201  \u51648 \u50948 \u45208  \u51473 \u50836 \u46020 \u47484  \u45208 \u53440 \u45240 \u45796 \u44256  \u49444 \u47749 \u54620 \u45796 .",\
          "5": "\uc0\u54617 \u49373 \u51008  \u49440 \u49373 \u45784 \u51032  \u49444 \u47749 \u51012  \u46307 \u44256  \u50715 \u45216  \u44536 \u47548 \u50640  \u45824 \u54620  \u55141 \u48120 \u47484  \u51075 \u45716 \u45796 ."\
        \},\
        "answer": "5",\
        "score": 2\
      \},\
      \{\
        "question": "\uc0\u50948  \u45824 \u54868 \u50640 \u49436  \u49440 \u49373 \u45784 \u51032  \u47568 \u54616 \u44592  \u48169 \u49885 \u50640  \u45824 \u54620  \u54617 \u49373 \u51032  \u48152 \u51025 \u51004 \u47196  \u44032 \u51109  \u51201 \u51208 \u54620  \u44163 \u51008 ? [3\u51216 ]",\
        "choices": \{\
          "1": "\uc0\u49440 \u49373 \u45784 \u51032  \u49444 \u47749 \u51060  \u45320 \u47924  \u50612 \u47140 \u50892 \u49436  \u51060 \u54644 \u54616 \u44592  \u55192 \u46308 \u50612 \u50836 .",\
          "2": "\uc0\u49440 \u49373 \u45784 \u51032  \u49444 \u47749  \u45909 \u48516 \u50640  \u50715 \u45216  \u44536 \u47548 \u51012  \u48372 \u45716  \u49352 \u47196 \u50868  \u49884 \u44033 \u51012  \u44054 \u44172  \u46104 \u50632 \u50612 \u50836 .",\
          "3": "\uc0\u49440 \u49373 \u45784 \u51032  \u49444 \u47749 \u51060  \u52628 \u49345 \u51201 \u51060 \u50612 \u49436  \u50715 \u45216  \u44536 \u47548 \u51032  \u53945 \u51669 \u51012  \u51096  \u47784 \u47476 \u44192 \u50612 \u50836 .",\
          "4": "\uc0\u49440 \u49373 \u45784 \u51032  \u49444 \u47749 \u51060  \u51109 \u54889 \u54644 \u49436  \u50715 \u45216  \u44536 \u47548 \u51032  \u54645 \u49900  \u45236 \u50857 \u51012  \u54028 \u50501 \u54616 \u44592  \u50612 \u47140 \u50892 \u50836 .",\
          "5": "\uc0\u49440 \u49373 \u45784 \u51032  \u49444 \u47749 \u51060  \u45436 \u47532 \u51201 \u51060 \u51648  \u50506 \u50500 \u49436  \u50715 \u45216  \u44536 \u47548 \u51012  \u50780  \u44536 \u47111 \u44172  \u44536 \u47160 \u45716 \u51648  \u51032 \u47928 \u51060 \u50640 \u50836 ."\
        \},\
        "answer": "2",\
        "score": 3\
      \}\
    ]\
  \},\
  \{\
    "id": "2020_11_KICE_24-27",\
    "paragraph": "\uc0\u49324 \u54924 \u51088 : \u50504 \u45397 \u54616 \u49901 \u45768 \u44620 ? \u50724 \u45720  \u53664 \u47200  \u51452 \u51228 \u45716  \'91\u46041 \u47932 \u50896 \u51008  \u51316 \u51116 \u54644 \u50556  \u54616 \u45716 \u44032 ?\'92\u51077 \u45768 \u45796 . \u52268 \u49457  \u52769 \u44284  \u48152 \u45824  \u52769 \u51008  \u44033  \u54016 \u51032  \u51452 \u51109 \u51012  3\u48516  \u51060 \u45236 \u47196  \u48157 \u54784  \u51452 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u47676 \u51200  \u52268 \u49457  \u52769 \u51032  \u51077 \u51109 \u51012  \u46308 \u50612  \u48372 \u44192 \u49845 \u45768 \u45796 . \u52268 \u49457  \u52769 : \u50504 \u45397 \u54616 \u49901 \u45768 \u44620 ? \u51200 \u55148 \u45716  \u46041 \u47932 \u50896 \u51060  \u51316 \u51116 \u54644 \u50556  \u54620 \u45796 \u45716  \u51077 \u51109 \u51077 \u45768 \u45796 . \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u51012  \u48372 \u54840 \u54616 \u44256  \u50672 \u44396 \u54616 \u45716  \u51473 \u50836 \u54620  \u50669 \u54624 \u51012  \u54633 \u45768 \u45796 . \u47736 \u51333  \u50948 \u44592 \u50640  \u52376 \u54620  \u46041 \u47932 \u46308 \u51012  \u46041 \u47932 \u50896 \u50640 \u49436  \u48372 \u54840 \u54616 \u44256  \u48264 \u49885 \u49884 \u53020  \u50556 \u49373 \u51004 \u47196  \u46028 \u47140 \u48372 \u45236 \u45716  \u45432 \u47141 \u51012  \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620  \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u50640  \u45824 \u54620  \u44368 \u50977 \u44284  \u50672 \u44396 \u47484  \u50948 \u54620  \u44277 \u44036 \u51077 \u45768 \u45796 . \u46041 \u47932 \u50896 \u50640 \u49436  \u46041 \u47932 \u51032  \u49373 \u53468 \u47484  \u44288 \u52272 \u54616 \u44256  \u50672 \u44396 \u54616 \u50668  \u46041 \u47932  \u48373 \u51648 \u50752  \u49373 \u47932  \u45796 \u50577 \u49457  \u48372 \u51316 \u50640  \u44592 \u50668 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49324 \u54924 \u51088 : \u48152 \u45824  \u52769 \u51008  \u50612 \u46523 \u44172  \u49373 \u44033 \u54616 \u49884 \u45208 \u50836 ? \u48152 \u45824  \u52769 : \u51200 \u55148 \u45716  \u46041 \u47932 \u50896 \u51060  \u51316 \u51116 \u54644 \u49436 \u45716  \u50504  \u46108 \u45796 \u45716  \u51077 \u51109 \u51077 \u45768 \u45796 . \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u51032  \u51088 \u50672 \u51201 \u51064  \u49845 \u49457 \u51012  \u51228 \u54620 \u54616 \u44256  \u46041 \u47932 \u50640 \u44172  \u49828 \u53944 \u47112 \u49828 \u47484  \u51469 \u45768 \u45796 . \u46041 \u47932 \u50896 \u51032  \u51329 \u51008  \u44277 \u44036 \u51008  \u50556 \u49373  \u46041 \u47932 \u51032  \u54876 \u46041 \u47049 \u51012  \u51228 \u54620 \u54616 \u44256 , \u51064 \u44277 \u51201 \u51064  \u54872 \u44221 \u51008  \u46041 \u47932 \u51032  \u49828 \u53944 \u47112 \u49828 \u47484  \u50976 \u48156 \u54616 \u50668  \u51221 \u49888 \u51201 \u51064  \u47928 \u51228 \u47484  \u51068 \u51004 \u53420  \u49688  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620  \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u51012  \u51204 \u49884 \u54616 \u45716  \u54665 \u50948 \u47484  \u53685 \u54644  \u46041 \u47932 \u51012  \u51064 \u44036 \u51032  \u51600 \u44144 \u50880 \u51012  \u50948 \u54620  \u46020 \u44396 \u47196  \u51060 \u50857 \u54633 \u45768 \u45796 . \u51060 \u45716  \u46041 \u47932 \u51032  \u51316 \u50628 \u49457 \u51012  \u55036 \u49552 \u54616 \u45716  \u54665 \u50948 \u51077 \u45768 \u45796 . \u49324 \u54924 \u51088 : \u51060 \u51228  \u50577  \u54016 \u51008  \u49345 \u45824  \u54016 \u51032  \u51452 \u51109 \u51012  \u48152 \u48149 \u54616 \u44256  \u51088 \u49888 \u51032  \u51452 \u51109 \u51012  \u50745 \u54840 \u54644  \u51452 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u47676 \u51200  \u52268 \u49457  \u52769 \u48512 \u53552  \u51656 \u47928 \u54644  \u51452 \u49464 \u50836 . \u52268 \u49457  \u52769 : \u48152 \u45824  \u52769 \u50640 \u49436 \u45716  \u46041 \u47932 \u50896 \u51060  \u46041 \u47932 \u50640 \u44172  \u49828 \u53944 \u47112 \u49828 \u47484  \u51456 \u45796 \u44256  \u51452 \u51109 \u54664 \u49845 \u45768 \u45796 . \u54616 \u51648 \u47564  \u54788 \u45824 \u51032  \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u51032  \u48373 \u51648 \u47484  \u44256 \u47140 \u54616 \u50668  \u46041 \u47932 \u50640 \u44172  \u52572 \u45824 \u54620  \u51088 \u50672 \u44284  \u50976 \u49324 \u54620  \u54872 \u44221 \u51012  \u51312 \u49457 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612 , \u46041 \u47932 \u51032  \u49436 \u49885 \u51648 \u50752  \u50976 \u49324 \u54620  \u54872 \u44221 \u51012  \u47564 \u46308 \u44256 , \u45796 \u50577 \u54620  \u54665 \u46041  \u54413 \u48512 \u54868  \u54532 \u47196 \u44536 \u47016 \u51012  \u51228 \u44277 \u54616 \u50668  \u46041 \u47932 \u51032  \u49828 \u53944 \u47112 \u49828 \u47484  \u51460 \u51060 \u44592  \u50948 \u54644  \u45432 \u47141 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u48152 \u45824  \u52769 : \u47932 \u47200  \u46041 \u47932 \u51032  \u48373 \u51648 \u47484  \u44256 \u47140 \u54616 \u45716  \u46041 \u47932 \u50896 \u46020  \u51080 \u51648 \u47564 , \u47784 \u46304  \u46041 \u47932 \u50896 \u51060  \u44536 \u47088  \u44163 \u51008  \u50500 \u45785 \u45768 \u45796 . \u51068 \u48512  \u46041 \u47932 \u50896 \u50640 \u49436 \u45716  \u50668 \u51204 \u55176  \u46041 \u47932 \u51012  \u51329 \u51008  \u44277 \u44036 \u50640  \u44032 \u46160 \u44256 , \u51064 \u50948 \u51201 \u51064  \u54872 \u44221 \u50640  \u45432 \u52636 \u49884 \u53020  \u46041 \u47932 \u50640 \u44172  \u49828 \u53944 \u47112 \u49828 \u47484  \u51452 \u44256  \u51080 \u49845 \u45768 \u45796 . \u49324 \u54924 \u51088 : \u48152 \u45824  \u52769 \u50640 \u49436  \u45796 \u49884  \u51656 \u47928 \u54644  \u51452 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u48152 \u45824  \u52769 : \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u51012  \u51204 \u49884 \u54616 \u45716  \u54665 \u50948 \u47484  \u53685 \u54644  \u46041 \u47932 \u51012  \u51064 \u44036 \u51032  \u51600 \u44144 \u50880 \u51012  \u50948 \u54620  \u46020 \u44396 \u47196  \u51060 \u50857 \u54620 \u45796 \u44256  \u51452 \u51109 \u54664 \u49845 \u45768 \u45796 . \u52268 \u49457  \u52769 : \u46041 \u47932 \u50896 \u51008  \u45800 \u49692 \u55176  \u46041 \u47932 \u51012  \u51204 \u49884 \u54616 \u45716  \u44275 \u51060  \u50500 \u45785 \u45768 \u45796 . \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u50640  \u45824 \u54620  \u44368 \u50977 \u44284  \u50672 \u44396 \u47484  \u53685 \u54644  \u49373 \u47932  \u45796 \u50577 \u49457  \u48372 \u51316 \u50640  \u44592 \u50668 \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612 , \u46041 \u47932 \u50896 \u50640 \u49436  \u47736 \u51333  \u50948 \u44592 \u50640  \u52376 \u54620  \u46041 \u47932 \u51012  \u48372 \u54840 \u54616 \u44256  \u48264 \u49885 \u49884 \u53020  \u50556 \u49373 \u51004 \u47196  \u46028 \u47140 \u48372 \u45236 \u45716  \u45432 \u47141 \u51012  \u54616 \u44256  \u51080 \u49845 \u45768 \u45796 . \u49324 \u54924 \u51088 : \u50577  \u54016  \u47784 \u46160  \u51339 \u51008  \u51032 \u44204 \u51012  \u51228 \u49884 \u54644  \u51452 \u49512 \u49845 \u45768 \u45796 . \u51060 \u51228  \u47560 \u47924 \u47532  \u48156 \u50616 \u51012  \u54644  \u51452 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u47676 \u51200  \u52268 \u49457  \u52769 \u48512 \u53552  \u47568 \u50432 \u54644  \u51452 \u49464 \u50836 . \u52268 \u49457  \u52769 : \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u51012  \u48372 \u54840 \u54616 \u44256  \u50672 \u44396 \u54616 \u45716  \u51473 \u50836 \u54620  \u50669 \u54624 \u51012  \u54633 \u45768 \u45796 . \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u50640  \u45824 \u54620  \u44368 \u50977 \u44284  \u50672 \u44396 \u47484  \u53685 \u54644  \u49373 \u47932  \u45796 \u50577 \u49457  \u48372 \u51316 \u50640  \u44592 \u50668 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u47932 \u47200  \u46041 \u47932 \u50896 \u51060  \u46041 \u47932 \u50640 \u44172  \u49828 \u53944 \u47112 \u49828 \u47484  \u51452 \u45716  \u48512 \u48516 \u46020  \u51080 \u51648 \u47564 , \u46041 \u47932  \u48373 \u51648 \u47484  \u50948 \u54644  \u45432 \u47141 \u54616 \u45716  \u46041 \u47932 \u50896 \u46020  \u47566 \u49845 \u45768 \u45796 . \u49324 \u54924 \u51088 : \u48152 \u45824  \u52769 \u51032  \u47560 \u47924 \u47532  \u48156 \u50616 \u51012  \u46308 \u50612  \u48372 \u44192 \u49845 \u45768 \u45796 . \u48152 \u45824  \u52769 : \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u51032  \u51088 \u50672 \u51201 \u51064  \u49845 \u49457 \u51012  \u51228 \u54620 \u54616 \u44256  \u46041 \u47932 \u50640 \u44172  \u49828 \u53944 \u47112 \u49828 \u47484  \u51469 \u45768 \u45796 . \u46608 \u54620  \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u51012  \u51204 \u49884 \u54616 \u45716  \u54665 \u50948 \u47484  \u53685 \u54644  \u46041 \u47932 \u51012  \u51064 \u44036 \u51032  \u51600 \u44144 \u50880 \u51012  \u50948 \u54620  \u46020 \u44396 \u47196  \u51060 \u50857 \u54633 \u45768 \u45796 . \u46041 \u47932 \u50896 \u51008  \u46041 \u47932 \u51032  \u51316 \u50628 \u49457 \u51012  \u55036 \u49552 \u54616 \u45716  \u49884 \u49444 \u51077 \u45768 \u45796 . \u49324 \u54924 \u51088 : \u50724 \u45720  \u53664 \u47200 \u50640  \u52280 \u50668 \u54644  \u51452 \u49888  \u50577  \u54016 \u44760  \u44048 \u49324 \u46300 \u47549 \u45768 \u45796 .\
