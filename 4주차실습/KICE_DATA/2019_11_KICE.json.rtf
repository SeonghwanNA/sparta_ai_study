    "paragraph": "\uc0\u50668 \u47084 \u48516  \u50504 \u45397 \u54616 \u49464 \u50836 ? \u50500 \u52840 \u54663 \u49332  \u48169 \u49569 \u51032  \u51652 \u54665 \u51012  \u47585 \u51008  \u44032 \u49688  \u9675 \u9675 \u9675 \u51077 \u45768 \u45796 . \u48164 \u49324 \u51060  \u54200 \u50504 \u54616 \u44172  \u51452 \u47924 \u49512 \u45208 \u50836 ? \u50724 \u45720 \u51008  \u55192 \u52268  \u54616 \u47336 \u47484  \u49884 \u51089 \u54616 \u44592  \u50948 \u54644  \u49888 \u45208 \u45716  \u51020 \u50501 \u51004 \u47196  \u48169 \u49569 \u51012  \u49884 \u51089 \u54616 \u44192 \u49845 \u45768 \u45796 . \u9836 \u9834 (\u44221 \u53132 \u54620  \u51020 \u50501 ) \u55120 \u55120 , \u50612 \u46496 \u49464 \u50836 ? \u51104 \u51060  \u54869  \u45804 \u50500 \u45208 \u51648  \u50506 \u45208 \u50836 ? \u51020 \u50501 \u51012  \u46307 \u44256  \u45208 \u45768  \u44592 \u48516 \u51060  \u51339 \u50500 \u51648 \u45716 \u45936 \u50836 , \u50668 \u47084 \u48516 \u46020  \u44536 \u47111 \u51424 ? \u51088 , \u44536 \u47100  \u51060 \u51228  \u50668 \u47084 \u48516 \u51060  \u48372 \u45236  \u51452 \u49888  \u49324 \u50672 \u51012  \u49548 \u44060 \u54624  \u49884 \u44036 \u51077 \u45768 \u45796 . \u50724 \u45720  \u49548 \u44060 \u54624  \u49324 \u50672 \u51008  \'91\u9675 \u9675 \u9675 \'92\u45784 \u44760 \u49436  \u48372 \u45236  \u51452 \u49512 \u49845 \u45768 \u45796 . \'91\u9675 \u9675 \u9675 \'92\u45784 \u51032  \u49324 \u50672 , \u51228 \u44032  \u51649 \u51217  \u51069 \u50612  \u46300 \u47540 \u44172 \u50836 . \'93\u51200 \u45716  \u12896 (\u52828 \u44396 \u46308 \u44284 \u51032  \u50557 \u49549  \u49884 \u44036 \u50640  \u45734 \u45716  \u54200 )\u51077 \u45768 \u45796 . \u50500 \u47924 \u47532  \u49436 \u46168 \u47084 \u46020  \u54637 \u49345  \u45734 \u50612 \u49436  \u52828 \u44396 \u46308 \u50640 \u44172  \u48120 \u50504 \u54620  \u47560 \u51020 \u51060  \u46317 \u45768 \u45796 . \u50557 \u49549  \u49884 \u44036 \u51012  \u51648 \u53412 \u47140 \u44256  \u45432 \u47141 \u54616 \u45716 \u45936  \u51096 \u46104 \u51648  \u50506 \u50500  \u44256 \u48124 \u51077 \u45768 \u45796 .\'94 \u51020 , \'91\u9675 \u9675 \u9675 \'92\u45784 \u44760 \u49436 \u45716  \u50557 \u49549  \u49884 \u44036 \u50640  \u51088 \u51452  \u45734 \u50612 \u49436  \u44256 \u48124 \u51060 \u49884 \u44400 \u50836 . \u50557 \u49549  \u49884 \u44036 \u50640  \u45734 \u51004 \u47732  \u52828 \u44396 \u46308 \u44284 \u51032  \u44288 \u44228 \u44032  \u50612 \u47140 \u50892 \u51656  \u49688  \u51080 \u51424 . \u52828 \u44396 \u46308 \u44284 \u51032  \u44288 \u44228 \u50640 \u49436  \u49888 \u47280 \u47484  \u51075 \u45716  \u44163 \u51008  \u51221 \u47568  \u50504 \u53440 \u44620 \u50868  \u51068 \u51077 \u45768 \u45796 . \u54616 \u51648 \u47564  \u45320 \u47924  \u44145 \u51221 \u54616 \u51648  \u47560 \u49464 \u50836 . \u50557 \u49549  \u49884 \u44036 \u50640  \u45734 \u45716  \u49845 \u44288 \u51008  \u52649 \u48516 \u55176  \u44256 \u52832  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50724 \u45720  \u51228 \u44032  \'91\u9675 \u9675 \u9675 \'92\u45784 \u44284  \u44057 \u51008  \u44256 \u48124 \u51012  \u54616 \u44256  \u51080 \u45716  \u48516 \u46308 \u51012  \u50948 \u54644  \u47751  \u44032 \u51648  \u48169 \u48277 \u51012  \u50508 \u47140  \u46300 \u47532 \u44192 \u49845 \u45768 \u45796 . \u52395  \u48264 \u51704 , \u50557 \u49549  \u49884 \u44036 \u51012  \u48120 \u47532  \u50508 \u47140  \u51452 \u45716  \u50508 \u46988  \u44592 \u45733 \u51012  \u54876 \u50857 \u54644  \u48372 \u49464 \u50836 . \u55092 \u45824  \u51204 \u54868 \u51032  \u50508 \u46988  \u44592 \u45733 \u51012  \u54876 \u50857 \u54616 \u47732  \u50557 \u49549  \u49884 \u44036 \u51012  \u51082 \u51648  \u50506 \u44256  \u52313 \u44600  \u49688  \u51080 \u49845 \u45768 \u45796 . \u50557 \u49549  \u49884 \u44036 \u48372 \u45796  30\u48516  \u46608 \u45716  1\u49884 \u44036  \u51204 \u50640  \u50508 \u46988 \u51012  \u49444 \u51221 \u54644  \u46160 \u47732  \u50557 \u49549  \u51109 \u49548 \u50640  \u45734 \u51648  \u50506 \u46020 \u47197  \u51456 \u48708 \u54624  \u49688  \u51080 \u44192 \u51424 ? \u46160  \u48264 \u51704 , \u50557 \u49549  \u51109 \u49548 \u44620 \u51648  \u44032 \u45716  \u45936  \u44152 \u47532 \u45716  \u49884 \u44036 \u51012  \u44228 \u49328 \u54644  \u48372 \u49464 \u50836 . \u51665 \u50640 \u49436  \u50557 \u49549  \u51109 \u49548 \u44620 \u51648  \u51060 \u46041 \u54616 \u45716  \u45936  \u50620 \u47560 \u45208  \u44152 \u47532 \u45716 \u51648  \u48120 \u47532  \u54869 \u51064 \u54616 \u45716  \u44163 \u51060  \u51473 \u50836 \u54633 \u45768 \u45796 . \u50696 \u49345 \u52824  \u47803 \u54620  \u49345 \u54889 \u51004 \u47196  \u45734 \u51012  \u49688 \u46020  \u51080 \u51004 \u45768 , \u50668 \u50976  \u51080 \u44172  \u49884 \u44036 \u51012  \u44228 \u49328 \u54616 \u45716  \u44163 \u51060  \u51339 \u44192 \u51424 . \u49464  \u48264 \u51704 , \u52828 \u44396 \u46308 \u50640 \u44172  \u45734 \u45716  \u51060 \u50976 \u47484  \u48120 \u47532  \u51060 \u50556 \u44592 \u54644  \u51452 \u45716  \u44163 \u51008  \u50612 \u46504 \u44620 \u50836 ? \u50557 \u49549  \u49884 \u44036 \u50640  \u45734 \u51012  \u44163  \u44057 \u51004 \u47732  \u52828 \u44396 \u46308 \u50640 \u44172  \u45734 \u45716  \u51060 \u50976 \u47484  \u48120 \u47532  \u51060 \u50556 \u44592 \u54616 \u45716  \u44163 \u51060  \u51339 \u49845 \u45768 \u45796 . \u51060 \u50976 \u47484  \u50508 \u47732  \u52828 \u44396 \u46308 \u51060  \u54868 \u47484  \u45236 \u44144 \u45208  \u45813 \u45813 \u54644 \u54616 \u51648  \u50506 \u44192 \u51424 ? \u51088 , \u51060  \u49464  \u44032 \u51648  \u48169 \u48277 \u51012  \u44592 \u50613 \u54616 \u44256  \u49892 \u52380 \u54620 \u45796 \u47732  \'91\u9675 \u9675 \u9675 \'92\u45784 \u52376 \u47100  \u50557 \u49549  \u49884 \u44036 \u50640  \u45734 \u45716  \u49845 \u44288 \u51012  \u44256 \u52832  \u49688  \u51080 \u51012  \u44144 \u50696 \u50836 . \u47560 \u51648 \u47561 \u51004 \u47196  \u45432 \u47000  \u54620  \u44257  \u46307 \u44256  \u47560 \u47924 \u47532 \u54616 \u44192 \u49845 \u45768 \u45796 . \u50724 \u45720  \u49548 \u44060 \u46108  \u49324 \u50672 \u51032  \u49888 \u52397 \u51088 \u50640 \u44172 \u45716  \u51200 \u51032  \u49324 \u51064  CD\u50752  \u49548 \u51221 \u51032  \u49345 \u54408 \u51012  \u48372 \u45236  \u46300 \u47549 \u45768 \u45796 . \u9836 \u9834 (\u44221 \u53132 \u54620  \u51020 \u50501 )",\
    "type": 0,\
    "problems": [\
      \{\
        "question": "\uc0\u50948  \u48169 \u49569  \u51652 \u54665 \u51088 \u51032  \u47568 \u54616 \u44592  \u48169 \u49885 \u50640  \u45824 \u54620  \u49444 \u47749 \u51004 \u47196  \u44032 \u51109  \u51201 \u51208 \u54620  \u44163 \u51008 ?",\
        "choices": \{\
          "1": "\uc0\u49324 \u50672  \u45236 \u50857 \u51012  \u50836 \u50557 \u54616 \u44256  \u49324 \u50672  \u49888 \u52397 \u51088 \u51032  \u51077 \u51109 \u51012  \u51648 \u51648 \u54616 \u44256  \u51080 \u45796 .",\
          "2": "\uc0\u49324 \u50672  \u49888 \u52397 \u51088 \u51032  \u47928 \u51228  \u49345 \u54889 \u51012  \u48516 \u49437 \u54616 \u44256  \u51204 \u47928 \u44032 \u51032  \u51032 \u44204 \u51012  \u51228 \u49884 \u54616 \u44256  \u51080 \u45796 .",\
          "3": "\uc0\u49324 \u50672 \u44284  \u44288 \u47144 \u46108  \u51088 \u49888 \u51032  \u44221 \u54744 \u51012  \u49548 \u44060 \u54616 \u44256  \u52397 \u52712 \u51088 \u50640 \u44172  \u44277 \u44048 \u51012  \u54364 \u54788 \u54616 \u44256  \u51080 \u45796 .",\
          "4": "\uc0\u49324 \u50672  \u49549  \u47928 \u51228  \u49345 \u54889 \u50640  \u45824 \u54620  \u54644 \u44208 \u52293 \u51012  \u51228 \u49884 \u54616 \u44256  \u52397 \u52712 \u51088 \u51032  \u52280 \u50668 \u47484  \u50976 \u46020 \u54616 \u44256  \u51080 \u45796 .",\
          "5": "\uc0\u49324 \u50672  \u49549  \u51064 \u47932 \u51032  \u49900 \u47532 \u47484  \u48516 \u49437 \u54616 \u44256  \u47928 \u51228  \u54644 \u44208 \u51012  \u50948 \u54620  \u51312 \u50616 \u51012  \u51228 \u49884 \u54616 \u44256  \u51080 \u45796 ."\
        \},\
        "answer": "1",\
        "score": 2\
      \},\
      \{\
        "question": "\uc0\u45796 \u51020 \u51008  \u50948  \u48169 \u49569 \u51012  \u50948 \u54620  \u48169 \u49569  \u44228 \u54925 \u49436 \u51032  \u51068 \u48512 \u51060 \u45796 . \u44228 \u54925 \u49436  \u45236 \u50857 \u50640  \u46300 \u47084 \u45208 \u51648  \u50506 \u51008  \u44163 \u51008 ?",\
        "choices": \{\
          "1": "\uc0\u48169 \u49569  \u51228 \u47785  \u48143  \u51652 \u54665 \u51088  \u49548 \u44060 ",\
          "2": "\uc0\u49324 \u50672 \u51032  \u54645 \u49900  \u45236 \u50857  \u49548 \u44060  \u48143  \u47928 \u51228  \u49345 \u54889  \u48516 \u49437 ",\
          "3": "\uc0\u49324 \u50672 \u44284  \u44288 \u47144 \u46108  \u44284 \u44144  \u48169 \u49569  \u45236 \u50857  \u49548 \u44060 ",\
          "4": "\uc0\u54644 \u44208 \u52293  \u51228 \u49884  \u48143  \u44396 \u52404 \u51201 \u51064  \u49324 \u47168  \u51228 \u49884 ",\
          "5": "\uc0\u45796 \u51020  \u48169 \u49569  \u50696 \u44256  \u48143  \u47560 \u47924 \u47532  \u51064 \u49324 "\
        \},\
        "answer": "3",\
        "score": 2\
      \},\
      \{\
        "question": "\uc0\u45796 \u51020 \u51008  \u50948  \u48169 \u49569 \u51012  \u46308 \u51008  \u52397 \u52712 \u51088 \u46308 \u51060  \u48169 \u49569  \u44172 \u49884 \u54032 \u50640  \u45224 \u44596  \u44544 \u51060 \u45796 . \u48169 \u49569  \u45236 \u50857 \u51012  \u44256 \u47140 \u54616 \u50668  \u52397 \u52712 \u51088 \u46308 \u51032  \u48152 \u51025 \u51012  \u48516 \u49437 \u54620  \u44163 \u51004 \u47196  \u51201 \u51208 \u54616 \u51648  \u50506 \u51008  \u44163 \u51008 ? [3\u51216 ]",\
        "choices": \{\
          "1": "\uc0\u52397 \u52712 \u51088  1\u51008  \u48169 \u49569  \u45236 \u50857 \u50640  \u44277 \u44048 \u54616 \u47728  \u51088 \u49888 \u51032  \u44221 \u54744 \u51012  \u46496 \u50732 \u47532 \u44256  \u51080 \u44400 .",\
          "2": "\uc0\u52397 \u52712 \u51088  2\u45716  \u48169 \u49569  \u45236 \u50857 \u51012  \u48148 \u53461 \u51004 \u47196  \u49324 \u50672  \u49549  \u51064 \u47932 \u51032  \u54665 \u46041 \u51012  \u50696 \u52769 \u54616 \u44256  \u51080 \u44400 .",\
          "3": "\uc0\u52397 \u52712 \u51088  3\u51008  \u48169 \u49569 \u50640 \u49436  \u51228 \u49884 \u46108  \u54644 \u44208 \u52293 \u50640  \u45824 \u54644  \u51032 \u47928 \u51012  \u51228 \u44592 \u54616 \u44256  \u51080 \u44400 .",\
          "4": "\uc0\u52397 \u52712 \u51088  4\u45716  \u48169 \u49569  \u45236 \u50857 \u51012  \u51088 \u49888 \u51032  \u49345 \u54889 \u50640  \u51201 \u50857 \u54644  \u48372 \u44256 \u51088  \u54616 \u45716  \u51032 \u51648 \u47484  \u46300 \u47084 \u45236 \u44256  \u51080 \u44400 .",\
          "5": "\uc0\u52397 \u52712 \u51088  5\u45716  \u48169 \u49569 \u50640 \u49436  \u49548 \u44060 \u46108  \u49324 \u50672 \u44284  \u50976 \u49324 \u54620  \u49324 \u47168 \u47484  \u46308 \u50612  \u51088 \u49888 \u51032  \u49373 \u44033 \u51012  \u48157 \u55176 \u44256  \u51080 \u44400 ."\
        \},\
        "answer": "3",\
        "score": 3\
      \}\
    ]\
  \},\
  \{\
    "id": "2019_11_KICE_4-7",\
    "paragraph": "\uc0\u49324 \u54924 \u51088  : \u50504 \u45397 \u54616 \u49464 \u50836 ? \u50724 \u45720  \u53664 \u47200  \u51452 \u51228 \u45716  \'91\u52397 \u49548 \u45380 \u51032  \u44172 \u51076  \u51060 \u50857  \u49884 \u44036 , \u51228 \u54620 \u54644 \u50556  \u54616 \u45716 \u44032 ?\'92\u51077 \u45768 \u45796 . \u52268 \u49457  \u52769 \u44284  \u48152 \u45824  \u52769 \u51008  \u44033  \u54016 \u51032  \u51452 \u51109 \u51012  3\u48516  \u51060 \u45236 \u47196  \u48157 \u54784  \u51452 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u47676 \u51200  \u52268 \u49457  \u52769 \u51032  \u51077 \u51109 \u51012  \u46308 \u50612  \u48372 \u44192 \u49845 \u45768 \u45796 . \u54617 \u49373  1 : \u50504 \u45397 \u54616 \u49464 \u50836 ? \u51200 \u45716  \u52268 \u49457  \u52769  \u51077 \u51109 \u51012  \u48157 \u55176 \u44192 \u49845 \u45768 \u45796 . \u44172 \u51076 \u51012  \u51648 \u45208 \u52824 \u44172  \u51060 \u50857 \u54616 \u47732  \u54617 \u50629 \u50640  \u51648 \u51109 \u51012  \u51452 \u44256  \u44148 \u44053 \u50640 \u46020  \u54644 \u47196 \u50872  \u49688  \u51080 \u49845 \u45768 \u45796 . \u46384 \u46972 \u49436  \u52397 \u49548 \u45380 \u51032  \u44172 \u51076  \u51060 \u50857  \u49884 \u44036 \u51012  \u51228 \u54620 \u54644 \u50556  \u54633 \u45768 \u45796 . \u49324 \u54924 \u51088  : \u48152 \u45824  \u52769 \u51008  \u50612 \u46523 \u44172  \u49373 \u44033 \u54616 \u49884 \u45208 \u50836 ? \u54617 \u49373  2 : \u51200 \u45716  \u48152 \u45824  \u52769  \u51077 \u51109 \u51012  \u47568 \u50432 \u46300 \u47532 \u44192 \u49845 \u45768 \u45796 . \u44172 \u51076 \u51012  \u53685 \u54644  \u49828 \u53944 \u47112 \u49828 \u47484  \u54644 \u49548 \u54616 \u44256  \u47928 \u51228  \u54644 \u44208  \u45733 \u47141 \u51012  \u53412 \u50872  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52397 \u49548 \u45380 \u44592 \u50640  \u44172 \u51076 \u51012  \u51600 \u44592 \u45716  \u44163 \u51008  \u51088 \u50672 \u49828 \u47084 \u50868  \u51068 \u51060 \u47728 , \u44172 \u51076  \u51060 \u50857  \u49884 \u44036 \u51012  \u51228 \u54620 \u54624  \u54596 \u50836 \u45716  \u50630 \u49845 \u45768 \u45796 . \u49324 \u54924 \u51088  : (\u51088 \u47308  \u51228 \u49884 ) \u51060 \u51228  \u50577  \u54016 \u51008  \u51228 \u44032  \u51228 \u49884 \u54620  \u51088 \u47308 \u47484  \u52280 \u44256 \u54616 \u50668  \u49345 \u45824  \u54016 \u51032  \u51452 \u51109 \u51012  \u48152 \u48149 \u54616 \u44256  \u51088 \u49888 \u51032  \u51452 \u51109 \u51012  \u50745 \u54840 \u54644  \u51452 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u47676 \u51200  \u52268 \u49457  \u52769 \u48512 \u53552  \u51656 \u47928 \u54644  \u51452 \u49464 \u50836 . \u54617 \u49373  1 : \u48152 \u45824  \u52769 \u50640 \u49436 \u45716  \u44172 \u51076 \u51012  \u53685 \u54644  \u49828 \u53944 \u47112 \u49828 \u47484  \u54644 \u49548 \u54624  \u49688  \u51080 \u45796 \u44256  \u51452 \u51109 \u54664 \u49845 \u45768 \u45796 . \u54616 \u51648 \u47564  \u44172 \u51076 \u51004 \u47196  \u51064 \u54644  \u49828 \u53944 \u47112 \u49828 \u47484  \u48155 \u45716  \u44221 \u50864 \u46020  \u47566 \u49845 \u45768 \u45796 . \u44172 \u51076 \u51012  \u54616 \u45796 \u44032  \u54868 \u44032  \u45208 \u44144 \u45208  \u51340 \u51208 \u44048 \u51012  \u45712 \u45180 \u45716  \u44221 \u50864 \u47484  \u50696 \u47196  \u46308  \u49688  \u51080 \u49845 \u45768 \u45796 . \u54617 \u49373  2 : \u47932 \u47200  \u44536 \u47088  \u44221 \u50864 \u46020  \u51080 \u49845 \u45768 \u45796 . \u54616 \u51648 \u47564  \u44172 \u51076 \u51012  \u53685 \u54644  \u49828 \u53944 \u47112 \u49828 \u47484  \u54644 \u49548 \u54616 \u45716  \u44221 \u50864 \u44032  \u45908  \u47566 \u45796 \u44256  \u49373 \u44033 \u54633 \u45768 \u45796 . \u44172 \u51076 \u51012  \u54616 \u47732 \u49436  \u52828 \u44396 \u46308 \u44284  \u54632 \u44760  \u51600 \u44144 \u50868  \u49884 \u44036 \u51012  \u48372 \u45236 \u44144 \u45208 , \u50612 \u47140 \u50868  \u47928 \u51228 \u47484  \u54644 \u44208 \u54616 \u47732 \u49436  \u49457 \u52712 \u44048 \u51012  \u45712 \u45188  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u49324 \u54924 \u51088  : \u52268 \u49457  \u52769 \u50640 \u49436  \u45796 \u49884  \u51656 \u47928 \u54644  \u51452 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u54617 \u49373  1 : \u44172 \u51076 \u51012  \u51648 \u45208 \u52824 \u44172  \u51060 \u50857 \u54616 \u47732  \u54617 \u50629 \u50640  \u51648 \u51109 \u51012  \u51460  \u49688  \u51080 \u45796 \u45716  \u44163 \u51008  \u48516 \u47749 \u54620  \u49324 \u49892 \u51077 \u45768 \u45796 . \u44172 \u51076 \u50640  \u48736 \u51256 \u49436  \u54617 \u44368  \u49688 \u50629 \u50640  \u51665 \u51473 \u54616 \u51648  \u47803 \u54616 \u44144 \u45208 , \u49689 \u51228 \u47484  \u51228 \u45824 \u47196  \u54616 \u51648  \u47803 \u54616 \u45716  \u44221 \u50864 \u44032  \u47566 \u49845 \u45768 \u45796 . \u54617 \u49373  2 : \u44172 \u51076 \u51060  \u54617 \u50629 \u50640  \u51648 \u51109 \u51012  \u51452 \u45716  \u44221 \u50864 \u46020  \u51080 \u51648 \u47564 , \u44172 \u51076 \u51012  \u53685 \u54644  \u54617 \u49845  \u45733 \u47141 \u51012  \u54693 \u49345 \u49884 \u53420  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u50696 \u47484  \u46308 \u50612 , \u51204 \u47029  \u49884 \u48044 \u47112 \u51060 \u49496  \u44172 \u51076 \u51012  \u53685 \u54644  \u47928 \u51228  \u54644 \u44208  \u45733 \u47141 \u44284  \u48708 \u54032 \u51201  \u49324 \u44256  \u45733 \u47141 \u51012  \u53412 \u50872  \u49688  \u51080 \u49845 \u45768 \u45796 . \u49324 \u54924 \u51088  : \u50577  \u54016  \u47784 \u46160  \u51339 \u51008  \u51032 \u44204 \u51012  \u51228 \u49884 \u54644  \u51452 \u49512 \u49845 \u45768 \u45796 . \u51060 \u51228  \u47560 \u47924 \u47532  \u48156 \u50616 \u51012  \u54644  \u51452 \u49884 \u44592  \u48148 \u46989 \u45768 \u45796 . \u47676 \u51200  \u52268 \u49457  \u52769 \u48512 \u53552  \u47568 \u50432 \u54644  \u51452 \u49464 \u50836 . \u54617 \u49373  1 : \u44172 \u51076 \u51008  \u51116 \u48120 \u51080 \u44256  \u50976 \u51061 \u54620  \u52769 \u47732 \u46020  \u51080 \u51648 \u47564 , \u51648 \u45208 \u52824 \u44172  \u51060 \u50857 \u54616 \u47732  \u50668 \u47084  \u44032 \u51648  \u47928 \u51228 \u47484  \u51068 \u51004 \u53420  \u49688  \u51080 \u49845 \u45768 \u45796 . \u52397 \u49548 \u45380 \u44592 \u50640  \u44172 \u51076 \u50640  \u51648 \u45208 \u52824 \u44172  \u47792 \u46160 \u54616 \u47732  \u54617 \u50629 \u50640  \u51648 \u51109 \u51012  \u51452 \u44256  \u44148 \u44053 \u50640 \u46020  \u54644 \u47196 \u50872  \u49688  \u51080 \u51004 \u48064 \u47196 , \u44172 \u51076  \u51060 \u50857  \u49884 \u44036 \u51012  \u51228 \u54620 \u54616 \u45716  \u44163 \u51060  \u54596 \u50836 \u54616 \u45796 \u44256  \u49373 \u44033 \u54633 \u45768 \u45796 . \u49324 \u54924 \u51088  : \u48152 \u45824  \u52769 \u51032  \u47560 \u47924 \u47532  \u48156 \u50616 \u51012  \u46308 \u50612  \u48372 \u44192 \u49845 \u45768 \u45796 . \u54617 \u49373  2 : \u44172 \u51076 \u51008  \u52397 \u49548 \u45380 \u46308 \u51060  \u49828 \u53944 \u47112 \u49828 \u47484  \u54644 \u49548 \u54616 \u44256  \u50668 \u44032  \u54876 \u46041 \u51012  \u51600 \u44600  \u49688  \u51080 \u45716  \u51339 \u51008  \u48169 \u48277 \u51077 \u45768 \u45796 . \u44172 \u51076 \u51012  \u53685 \u54644  \u45796 \u50577 \u54620  \u45733 \u47141 \u51012  \u53412 \u50872  \u49688 \u46020  \u51080 \u49845 \u45768 \u45796 . \u52397 \u49548 \u45380 \u51032  \u44172 \u51076  \u51060 \u50857  \u49884 \u44036 \u51012  \u51228 \u54620 \u54616 \u45716  \u44163 \u48372 \u45796  \u44148 \u44053 \u54620  \u44172 \u51076  \u51060 \u50857  \u49845 \u44288 \u51012  \u44592 \u47476 \u46020 \u47197  \u51648 \u46020 \u54616 \u45716  \u44163 \u51060  \u45908  \u51473 \u50836 \u54616 \u45796 \u44256  \u49373 \u44033 \u54633 \u45768 \u45796 .",\
