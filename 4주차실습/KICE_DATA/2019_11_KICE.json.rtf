  "3": "\uc0\u51921 \u51060 \u44032  \u53468 \u50612 \u45224  \u8594  \u49324 \u46988 \u46308 \u51060  \u51921 \u51060  \u50628 \u47560 \u47484  \u50504 \u51204 \u54620  \u44275 \u51004 \u47196  \u50734 \u44200  \u51468  \u8594  \u51921 \u51060  \u50628 \u47560 \u44032  \u51222 \u51012  \u47673 \u51060 \u51648  \u50506 \u44256  \u50612 \u46356 \u47200 \u44032  \u44048  \u8594  \u54624 \u47672 \u45768 \u44032  \u51921 \u51060 \u47484  \u48156 \u44204 \u54616 \u44256  \u51665 \u51004 \u47196  \u45936 \u47140 \u50740  \u8594  \u51921 \u51060 \u44032  \u51222 \u48337 \u51004 \u47196  \u50864 \u50976 \u47484  \u47673 \u51020 ",\
  "4": "\uc0\u51921 \u51060 \u44032  \u53468 \u50612 \u45224  \u8594  \u49324 \u46988 \u46308 \u51060  \u51921 \u51060  \u50628 \u47560 \u47484  \u50504 \u51204 \u54620  \u44275 \u51004 \u47196  \u50734 \u44200  \u51468  \u8594  \u54624 \u47672 \u45768 \u44032  \u51921 \u51060 \u47484  \u48156 \u44204 \u54616 \u44256  \u51665 \u51004 \u47196  \u45936 \u47140 \u50740  \u8594  \u51921 \u51060  \u50628 \u47560 \u44032  \u51222 \u51012  \u47673 \u51060 \u51648  \u50506 \u44256  \u50612 \u46356 \u47200 \u44032  \u44048  \u8594  \u51921 \u51060 \u44032  \u51222 \u48337 \u51004 \u47196  \u50864 \u50976 \u47484  \u47673 \u51020 ",\
  "5": "\uc0\u49324 \u46988 \u46308 \u51060  \u51921 \u51060  \u50628 \u47560 \u47484  \u50504 \u51204 \u54620  \u44275 \u51004 \u47196  \u50734 \u44200  \u51468  \u8594  \u51921 \u51060 \u44032  \u53468 \u50612 \u45224  \u8594  \u51921 \u51060  \u50628 \u47560 \u44032  \u51222 \u51012  \u47673 \u51060 \u51648  \u50506 \u44256  \u50612 \u46356 \u47200 \u44032  \u44048  \u8594  \u54624 \u47672 \u45768 \u44032  \u51921 \u51060 \u47484  \u48156 \u44204 \u54616 \u44256  \u51665 \u51004 \u47196  \u45936 \u47140 \u50740  \u8594  \u51921 \u51060 \u44032  \u51222 \u48337 \u51004 \u47196  \u50864 \u50976 \u47484  \u47673 \u51020 "\
