  "id": "2019_11_KICE_12-15",\
  "paragraph": "\uc0\u50715 \u45216  \u50612 \u45712  \u47560 \u51012 \u50640  \u45208 \u47924 \u44988 \u51060  \u49332 \u44256  \u51080 \u50632 \u49845 \u45768 \u45796 . \u50612 \u45712  \u45216  \u45208 \u47924 \u44988 \u51008  \u49328 \u50640  \u45208 \u47924 \u47484  \u54616 \u47084  \u44052 \u45796 \u44032  \u45931 \u50640  \u44152 \u47536  \u49324 \u49844 \u51012  \u48156 \u44204 \u54664 \u49845 \u45768 \u45796 . \u45208 \u47924 \u44988 \u51008  \u49324 \u49844 \u51012  \u48520 \u49933 \u55176  \u50668 \u44200  \u45931 \u50640 \u49436  \u54400 \u50612  \u51452 \u50632 \u49845 \u45768 \u45796 . \u44536 \u47084 \u51088  \u49324 \u49844 \u51008  \u45208 \u47924 \u44988 \u50640 \u44172  \u44256 \u47577 \u45796 \u45716  \u51064 \u49324 \u47484  \u54616 \u44256  \u49328 \u49549 \u51004 \u47196  \u49324 \u46972 \u51276 \u49845 \u45768 \u45796 . \u45796 \u51020  \u45216 , \u45208 \u47924 \u44988 \u51008  \u49328 \u50640 \u49436  \u45208 \u47924 \u47484  \u54616 \u44256  \u51080 \u45716 \u45936  \u50612 \u51228  \u54400 \u50612  \u51456  \u49324 \u49844 \u51060  \u45208 \u53440 \u45228 \u49845 \u45768 \u45796 . \u49324 \u49844 \u51008  \u45208 \u47924 \u44988 \u51012  \u49714 \u49549  \u44618 \u51008  \u44275 \u51004 \u47196  \u45936 \u47140 \u44052 \u49845 \u45768 \u45796 . \u44536 \u44275 \u50640 \u45716  \u44552 \u51004 \u47196  \u46108  \u51107 \u45208 \u47924 \u44032  \u51080 \u50632 \u49845 \u45768 \u45796 . \u49324 \u49844 \u51008  \u45208 \u47924 \u44988 \u50640 \u44172  \u44552  \u51107 \u51012  \u46384 \u49436  \u44032 \u51256 \u44032 \u46972 \u44256  \u54664 \u49845 \u45768 \u45796 . \u45208 \u47924 \u44988 \u51008  \u44552  \u51107 \u51012  \u46384 \u49436  \u51665 \u51004 \u47196  \u46028 \u50500 \u50752  \u48512 \u51088 \u44032  \u46104 \u50632 \u49845 \u45768 \u45796 . \u45208 \u47924 \u44988 \u51032  \u51060 \u50556 \u44592 \u47484  \u46308 \u51008  \u50837 \u49900 \u51137 \u51060  \u50689 \u44048 \u51008  \u49328 \u51004 \u47196  \u50732 \u46972 \u44032  \u45931 \u50640  \u44152 \u47536  \u49324 \u49844 \u51012  \u52286 \u50520 \u49845 \u45768 \u45796 . \u50689 \u44048 \u51008  \u49324 \u49844 \u51012  \u54400 \u50612  \u51452 \u45716  \u52377 \u54616 \u45796 \u44032  \u48167 \u51460 \u47196  \u47926 \u50612 \u49436  \u51665 \u51004 \u47196  \u45936 \u47140 \u50772 \u49845 \u45768 \u45796 . \u44536 \u47532 \u44256  \u49324 \u49844 \u50640 \u44172  \u44552  \u51107 \u45208 \u47924 \u44032  \u51080 \u45716  \u44275 \u51004 \u47196  \u45936 \u47140 \u44032  \u45804 \u46972 \u44256  \u54664 \u49845 \u45768 \u45796 . \u49324 \u49844 \u51008  \u50612 \u51796  \u49688  \u50630 \u51060  \u50689 \u44048 \u51012  \u49714 \u49549 \u51004 \u47196  \u45936 \u47140 \u44052 \u49845 \u45768 \u45796 . \u54616 \u51648 \u47564  \u44552  \u51107 \u45208 \u47924 \u44032  \u51080 \u45912  \u51088 \u47532 \u50640 \u45716  \u52964 \u45796 \u46976  \u50885 \u45929 \u51060 \u47564  \u51080 \u50632 \u49845 \u45768 \u45796 . \u50689 \u44048 \u51008  \u50885 \u45929 \u51060 \u50640  \u48736 \u51256  \u54728 \u50864 \u51201 \u44144 \u47160 \u49845 \u45768 \u45796 . \u44536 \u46412  \u49324 \u49844 \u51008  \u50689 \u44048 \u51012  \u50885 \u45929 \u51060 \u50640 \u49436  \u45132 \u50612 \u45236  \u51452 \u50632 \u49845 \u45768 \u45796 . \u50689 \u44048 \u51008  \u50837 \u49900 \u51012  \u48512 \u47536  \u44163 \u51012  \u54980 \u54924 \u54616 \u47728  \u49324 \u49844 \u50640 \u44172  \u44256 \u47577 \u45796 \u45716  \u51064 \u49324 \u47484  \u54616 \u44256  \u51665 \u51004 \u47196  \u46028 \u50500 \u44052 \u49845 \u45768 \u45796 .",\
