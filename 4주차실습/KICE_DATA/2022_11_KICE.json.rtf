    "paragraph": "\uc0\u49324 \u46988 \u46308 \u51008  \u55124 \u55176  \u50696 \u49696 \u51012  \u51221 \u49436 \u51201  \u44221 \u54744 \u44284  \u44208 \u48512 \u49884 \u53416 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51012  \u44048 \u49345 \u54624  \u46412  \u45712 \u45180 \u45716  \u51221 \u49436 \u51201  \u51600 \u44144 \u50880 \u51012  \u50696 \u49696 \u51032  \u48376 \u51656 \u51201 \u51064  \u53945 \u49457 \u51004 \u47196  \u50668 \u44592 \u45716  \u44163 \u51060 \u45796 . \u51060 \u47084 \u54620  \u44288 \u51216 \u50640 \u49436  \u50696 \u49696  \u51089 \u54408 \u51008  \u44048 \u49345 \u51088 \u50640 \u44172  \u51221 \u49436 \u51201  \u47564 \u51313 \u44048 \u51012  \u51452 \u45716  \u53132 \u46973 \u51032  \u50896 \u52380 \u51004 \u47196  \u44036 \u51452 \u46108 \u45796 . \u54616 \u51648 \u47564  \u50696 \u49696  \u51089 \u54408 \u51012  \u44048 \u49345 \u54624  \u46412  \u50864 \u47532 \u44032  \u45712 \u45180 \u45716  \u51600 \u44144 \u50880 \u51008  \u45800 \u49692 \u54620  \u53132 \u46973 \u51060 \u45208  \u50724 \u46973 \u51201  \u51600 \u44144 \u50880 \u44284 \u45716  \u44396 \u48324 \u46104 \u45716  \u49900 \u48120 \u51201  \u51600 \u44144 \u50880 \u51060 \u45796 . \u49900 \u48120 \u51201  \u51600 \u44144 \u50880 \u51008  \u50696 \u49696  \u51089 \u54408 \u51032  \u50500 \u47492 \u45796 \u50880 \u51060 \u45208  \u54805 \u49885 \u51201  \u50756 \u49457 \u46020 , \u46608 \u45716  \u51089 \u54408 \u50640  \u45812 \u44596  \u51032 \u48120 \u50752  \u44032 \u52824 \u47484  \u51060 \u54644 \u54616 \u44256  \u44048 \u49345 \u54616 \u45716  \u45936 \u49436  \u48708 \u47215 \u46104 \u45716  \u44256 \u52264 \u50896 \u51201 \u51064  \u51600 \u44144 \u50880 \u51060 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51008  \u44048 \u49345 \u51088 \u51032  \u51221 \u49436 \u51201  \u48152 \u51025 \u51012  \u51060 \u45132 \u50612  \u45244  \u49104 \u47564  \u50500 \u45768 \u46972 , \u44048 \u49345 \u51088 \u51032  \u49345 \u49345 \u47141 \u44284  \u51648 \u51201  \u45733 \u47141 \u51012  \u51088 \u44537 \u54616 \u50668  \u49352 \u47196 \u50868  \u44648 \u45804 \u51020 \u44284  \u53685 \u52272 \u51012  \u50619 \u44172  \u54620 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51008  \u44048 \u49345 \u51088 \u50640 \u44172  \u45800 \u49692 \u54620  \u51221 \u49436 \u51201  \u51600 \u44144 \u50880 \u51012  \u45336 \u50612  \u49900 \u50724 \u54620  \u49900 \u48120 \u51201  \u44221 \u54744 \u51012  \u51228 \u44277 \u54616 \u45716  \u44163 \u51060 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51012  \u44048 \u49345 \u54616 \u45716  \u44284 \u51221 \u50640 \u49436  \u50864 \u47532 \u45716  \u51089 \u54408 \u51032  \u50500 \u47492 \u45796 \u50880 \u44284  \u51032 \u48120 \u47484  \u53456 \u44396 \u54616 \u44256 , \u51089 \u44032 \u51032  \u51032 \u46020 \u50752  \u54364 \u54788  \u44592 \u48277 \u51012  \u48516 \u49437 \u54616 \u47728 , \u51089 \u54408 \u51060  \u45812 \u44256  \u51080 \u45716  \u49324 \u54924 \'b7\u47928 \u54868 \u51201  \u47589 \u46973 \u51012  \u51060 \u54644 \u54616 \u47140 \u44256  \u45432 \u47141 \u54620 \u45796 . \u46608 \u54620  \u50696 \u49696  \u51089 \u54408 \u51008  \u44048 \u49345 \u51088 \u51032  \u44060 \u51064 \u51201 \u51064  \u44221 \u54744 \u44284  \u44032 \u52824 \u44288 \u50640  \u46384 \u46972  \u45796 \u47476 \u44172  \u54644 \u49437 \u46112  \u49688  \u51080 \u51004 \u47728 , \u51060 \u47084 \u54620  \u54644 \u49437 \u51032  \u45796 \u50577 \u49457 \u51008  \u50696 \u49696  \u51089 \u54408 \u50640  \u45824 \u54620  \u44618 \u51060  \u51080 \u45716  \u51060 \u54644 \u47484  \u44032 \u45733 \u54616 \u44172  \u54620 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51008  \u44048 \u49345 \u51088 \u51032  \u51201 \u44537 \u51201 \u51064  \u52280 \u50668 \u50752  \u54644 \u49437 \u51012  \u53685 \u54644  \u49900 \u48120 \u51201  \u44221 \u54744 \u51012  \u54413 \u48512 \u54616 \u44172  \u47564 \u46308 \u50612  \u51452 \u45716  \u44163 \u51060 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51008  \u44048 \u49345 \u51088 \u51032  \u51221 \u49436 \u47484  \u51221 \u54868 \u54616 \u44256  \u44256 \u50577 \u49884 \u53412 \u45716  \u50669 \u54624 \u51012  \u54620 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51012  \u44048 \u49345 \u54616 \u47732 \u49436  \u50864 \u47532 \u45716  \u51068 \u49345 \u49373 \u54876 \u50640 \u49436  \u50613 \u45580 \u47160 \u45912  \u44048 \u51221 \u51012  \u54644 \u49548 \u54616 \u44256 , \u49352 \u47196 \u50868  \u44048 \u51221 \u51012  \u44221 \u54744 \u54616 \u47728 , \u51221 \u49888 \u51201 \u51064  \u54413 \u50836 \u47196 \u50880 \u51012  \u45712 \u45188  \u49688  \u51080 \u45796 . \u49836 \u54548 , \u44592 \u49256 , \u48516 \u45432 , \u50672 \u48124  \u46321  \u45796 \u50577 \u54620  \u44048 \u51221 \u51012  \u50696 \u49696  \u51089 \u54408 \u51012  \u53685 \u54644  \u44221 \u54744 \u54616 \u44256  \u54364 \u52636 \u54616 \u47732 \u49436  \u50864 \u47532 \u45716  \u51221 \u49436 \u51201 \u51064  \u50504 \u51221 \u44048 \u51012  \u50619 \u44256 , \u49334 \u51032  \u54876 \u47141 \u51012  \u46104 \u52286 \u51012  \u49688  \u51080 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51008  \u44048 \u49345 \u51088 \u51032  \u51221 \u49436 \u51201  \u44148 \u44053 \u50640  \u44557 \u51221 \u51201 \u51064  \u50689 \u54693 \u51012  \u48120 \u52824 \u45716  \u44163 \u51060 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51008  \u51064 \u44036 \u51032  \u49334 \u44284  \u44221 \u54744 \u51012  \u48152 \u50689 \u54616 \u44256 , \u49324 \u54924 \'b7\u47928 \u54868 \u51201  \u44032 \u52824 \u47484  \u45812 \u44256  \u51080 \u51004 \u47728 , \u49884 \u45824 \u47484  \u52488 \u50900 \u54616 \u50668  \u51064 \u44036 \u50640 \u44172  \u44048 \u46041 \u44284  \u44648 \u45804 \u51020 \u51012  \u51456 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51012  \u44048 \u49345 \u54616 \u45716  \u44163 \u51008  \u51064 \u44036 \u51032  \u49334 \u44284  \u49464 \u49345 \u51012  \u51060 \u54644 \u54616 \u44256 , \u49828 \u49828 \u47196 \u51032  \u45236 \u47732 \u51012  \u49457 \u52272 \u54616 \u47728 , \u49334 \u51032  \u51032 \u48120 \u47484  \u46104 \u49352 \u44592 \u45716  \u49900 \u50724 \u54620  \u44221 \u54744 \u51060 \u45796 . \u50696 \u49696  \u51089 \u54408 \u51008  \u51064 \u44036 \u51032  \u49334 \u51012  \u54413 \u50836 \u47213 \u44172  \u47564 \u46308 \u44256  \u51221 \u49888 \u51201 \u51064  \u49457 \u51109 \u51012  \u52489 \u51652 \u54616 \u45716  \u44163 \u51060 \u45796 .",\
