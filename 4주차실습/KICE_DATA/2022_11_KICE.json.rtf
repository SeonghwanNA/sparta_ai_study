          "3": "'\uc0\u51068 \u48512  \u44537 \u49548 \u49688 \u51032  \u49324 \u46988 \u46308 '\u50640 \u44172 \u45716  '\u47924 \u51204 \u44592 '\u47484  \u44032 \u51652  \u49324 \u46988 \u51004 \u47196  \u48372 \u51060 \u47140 \u45716  \u44608 \u45804 \u52292 \u51032  \u51032 \u46020 \u44032  \u49892 \u54788 \u46104 \u51648  \u50506 \u45716 \u45796 .",\
          "4": "\uc0\u44608 \u45804 \u52292 \u45716  '\u47924 \u51204 \u44592 '\u50640  \u51061 \u49689 \u54616 \u51648  \u50506 \u51008  '\u44144 \u44060 \u51032  \u49436 \u48124 \u46308 '\u51060  <\u48372 \u44592 >\u47484  '\u47924 \u51204 \u44592 '\u47196  \u50724 \u51064 \u54620 \u45796 \u44256  \u54032 \u45800 \u54620 \u45796 .",\
          "5": "'\uc0\u49324 \u48373  \u52264 \u47548 \u51032  \u52397 \u45380 '\u51008  '\u47924 \u51204 \u44592 '\u50640  \u51061 \u49689 \u54616 \u50668  <\u48372 \u44592 >\u47484  \u51060 \u50857 \u54616 \u47140 \u45716  \u44608 \u45804 \u52292 \u51032  \u51032 \u46020 \u47484  \u50508 \u50500 \u52296 \u45796 ."\
        \},\
        "answer": "5",\
        "score": 2\
      \},\
      \{\
        "question": "<\uc0\u48372 \u44592  1>\u51012  \u48148 \u53461 \u51004 \u47196  \u50967 \u44544 \u51012  \u44048 \u49345 \u54620  \u45236 \u50857 \u51004 \u47196  \u51201 \u51208 \u54616 \u51648  \u50506 \u51008  \u44163 \u51008 ? [3\u51216 ]",\
        "choices": \{\
          "1": "\uc0\u44608 \u45804 \u52292 \u44032  \u44033 \u44228 \u44033 \u52789  \u49324 \u46988 \u46308 \u51032  \u48152 \u51025 \u51012  \u46496 \u48372 \u45716  \u44163 \u51008 , \u44428 \u47141 \u51060  \u53440 \u51064 \u46308 \u50640 \u44172  \u48120 \u52824 \u45716  \u50689 \u54693 \u51012  \u49332 \u54592 \u45796 \u45716  \u51216 \u50640 \u49436  \u44608 \u45804 \u52292 \u44032  \u44428 \u47141 \u44288 \u44228 \u47484  \u51032 \u49885 \u54616 \u45716  \u51064 \u47932 \u51076 \u51012  \u46300 \u47084 \u45236 \u45716 \u44400 .",\
          "2": "\uc0\u44608 \u45804 \u52292 \u44032  \u51456  \u49696 \u44050 \u51012  \u54252 \u51109 \u47560 \u52283 \u51665  \u51452 \u51064 \u51060  \u48155 \u51648  \u50506 \u51004 \u47140 \u45716  \u44163 \u51008 , \u44428 \u47141 \u50640  \u45824 \u54620  \u49324 \u46988 \u46308 \u51032  \u53468 \u46020 \u47484  \u45208 \u53440 \u45240 \u45796 \u45716  \u51216 \u50640 \u49436  \u44428 \u47141 \u51060  \u51064 \u47932  \u44036 \u51032  \u50864 \u50676  \u44288 \u44228 \u47484  \u54805 \u49457 \u54616 \u45716  \u50836 \u51064 \u51076 \u51012  \u48372 \u50668  \u51452 \u45716 \u44400 .",\
          "3": "\uc0\u44608 \u45804 \u52292 \u44032  \u50808 \u50577 \u50640  \u48320 \u54868 \u47484  \u51456  \u44163 \u51008 , \u53440 \u51064 \u51032  \u49849 \u51064 \u51012  \u50857 \u51060 \u54616 \u44172  \u48155 \u51004 \u47140  \u54620 \u45796 \u45716  \u51216 \u50640 \u49436  \u54728 \u44396 \u51201  \u44428 \u47141  \u54364 \u51648 \u47484  \u51060 \u50857 \u54616 \u45716  \u45936  \u45908  \u51201 \u44537 \u51201 \u51004 \u47196  \u45208 \u49436 \u47140 \u45716  \u44608 \u45804 \u52292 \u51032  \u51032 \u46020 \u47484  \u45208 \u53440 \u45236 \u45716 \u44400 .",\
          "4": "\uc0\u44608 \u45804 \u52292 \u44032  \u49324 \u48373 \u46308 \u50640 \u44172  \u47785 \u52397 \u51012  \u45458 \u51060 \u47728  \u54637 \u51032 \u54616 \u45716  \u44163 \u51008 , \u51088 \u49888 \u46020  \u47784 \u47476 \u44172  \u50857 \u44592 \u47484  \u46300 \u47084 \u45256 \u45796 \u45716  \u51216 \u50640 \u49436  \u49849 \u51064 \u48155 \u51008  \u44221 \u54744 \u46308 \u51012  \u53685 \u54644  \u50619 \u44172  \u46108  \u44608 \u45804 \u52292 \u51032  \u51088 \u49888 \u44048 \u51012  \u48372 \u50668  \u51452 \u45716 \u44400 .",\
          "5": "\uc0\u44608 \u45804 \u52292 \u44032  \u48708 \u54364 \u47484  \u45800  \u52397 \u45380  \u50526 \u50640 \u49436  \u46028 \u50500 \u49436 \u45716  \u44163 \u51008 , \u54617 \u49373 \u46308 \u44284  \u47610 \u51008  \u50976 \u45824  \u44288 \u44228 \u47484  \u45800 \u51208 \u54616 \u50668  \u44592 \u46301 \u44428 \u51012  \u51648 \u53412 \u47140  \u54620 \u45796 \u45716  \u51216 \u50640 \u49436  \u49345 \u54889  \u45436 \u47532 \u47484  \u46384 \u47476 \u45716  \u44608 \u45804 \u52292 \u51032  \u53440 \u49328 \u51201  \u53468 \u46020 \u47484  \u46300 \u47084 \u45236 \u45716 \u44400 ."\
        \},\
        "answer": "5",\
        "score": 3\
      \}\
    ]\
  \},\
  \{\
    "id": "2022_11_KICE_28-31",\
    "paragraph": "\uc0\u49849 \u49345  \u45208 \u50629 \u51008  \u46392  \u54616 \u45208 \u44032  \u51080 \u50632 \u45796 . \u51116 \u50696 (\u25165 \u34269 )\u44032  \u45817 \u45824 \u50640  \u48764 \u50612 \u45228 \u45796 . \u50500 \u51060 \u45716  \u51060  \u47568 \u51012  \u46307 \u44256  \u54732  \u50743 \u51004 \u47196  \u44040 \u50500 \u51077 \u44256  \u44144 \u50872  \u44256 \u52824 \u45716  \u51109 \u49324 \u46972  \u49549 \u50668  \u49849 \u49345  \u51665  \u50526 \u50640  \u44032 \u49436  \\"\u44144 \u50872  \u44256 \u52824 \u49884 \u50724 !\\"\u46972  \u50808 \u52452 \u45796 . \u49548 \u51200 \u45716  \u51060  \u47568 \u51012  \u46307 \u44256  \u44144 \u50872 \u51012  \u44732 \u45236  \u50976 \u47784 \u50640 \u44172  \u51452 \u50612  \u48372 \u45256 \u45796 . \u49548 \u51200 \u45716  \u50976 \u47784  \u46244 \u47484  \u46384 \u46972  \u48148 \u44645 \u47928  \u50504 \u51901 \u44620 \u51648  \u45208 \u44032  \u47928 \u53960 \u51004 \u47196  \u50687 \u48372 \u50520 \u45796 . \u51109 \u49324 \u44032  \u49548 \u51200 \u51032  \u50620 \u44404 \u51012  \u50616 \u46907  \u48372 \u44256  \u48152 \u54644 , \u49552 \u50640  \u51536 \u50632 \u45912  \u44144 \u50872 \u51012  \u51068 \u48512 \u47084  \u46504 \u50612 \u46888 \u47140  \u44648 \u46888 \u47160 \u45796 . \u50976 \u47784 \u44032  \u45440 \u46972  \u54868 \u45236 \u47728  \u46412 \u47532 \u51088  \u51109 \u49324 \u44032  \u50872 \u47728  \u47568 \u54664 \u45796 . \\"\u44144 \u50872 \u51060  \u51060 \u48120  \u44648 \u51276 \u44144 \u45720  \u46412 \u47140  \u47924 \u50631  \u54616 \u49464 \u50836 ? \u51200 \u47484  \u45432 \u48708 \u47196  \u49340 \u50500  \u44144 \u50872  \u44050 \u51012  \u44058 \u44172  \u54644  \u51452 \u49464 \u50836 .\\" \u50976 \u47784 \u44032  \u46308 \u50612 \u44032  \u51060 \u47484  \u49849 \u49345 \u44760  \u50500 \u47280 \u45768  \u54728 \u46973 \u54616 \u50688 \u45796 . \u49849 \u49345 \u51008  \u44536 \u51032  \u51060 \u47492 \u51012  \u44144 \u50872 \u51012  \u44648 \u46888 \u47536  \u45432 \u48708 \u46972 \u45716  \u46907 \u51004 \u47196  \u54028 \u44221 \u45432 (\u30772 \u37857 \u22900 )\u46972  \u51667 \u44256  \u47568  \u47673 \u51060 \u45716  \u51068 \u51012  \u49884 \u53040 \u45796 . \u47568 \u46308 \u51008  \u51200 \u51208 \u47196  \u49332 \u52236 \u50612  \u50668 \u50952  \u44163 \u51060  \u54616 \u45208 \u46020  \u50630 \u50632 \u45796 . \u54616 \u47336 \u45716  \u52380 \u49345 \u51032  \u49440 \u44288 \u46308 \u51060  \u44396 \u47492 \u52376 \u47100  \u47792 \u47140 \u50752  \u47568  \u47673 \u51068  \u44852 \u51012  \u45796 \u53804 \u50612  \u44536 \u50640 \u44172  \u51452 \u50632 \u45796 . \u51060 \u50640  \u54028 \u44221 \u45432 \u45716  \u47568 \u46308 \u51012  \u54400 \u50612 \u45459 \u44256  \u45572 \u50892 \u47564  \u51080 \u50632 \u45796 . \u45216 \u51060  \u51200 \u47932 \u50612  \u47568 \u46308 \u51060  \u54028 \u44221 \u45432 \u44032  \u45572 \u50892  \u51080 \u45716  \u44275 \u50640  \u50752  \u44536 \u47484  \u54693 \u54644  \u47672 \u47532 \u47484  \u49689 \u51060 \u47728  \u45720 \u50612 \u49436 \u51088  \u48372 \u45716  \u51088 \u47560 \u45796  \u47784 \u46160  \u44592 \u51060 \u54616 \u44172  \u50668 \u44220 \u45796 . \u49849 \u49345  \u48512 \u51064 \u51008  \u51060  \u47568 \u51012  \u46307 \u44256  \u49849 \u49345 \u50640 \u44172  \u47568 \u54664 \u45796 . \\"\u54028 \u44221 \u45432 \u45716  \u50857 \u47784 \u44032  \u44592 \u51060 \u54616 \u44256  \u53444 \u48373 \u54624  \u51068 \u51060  \u47566 \u51004 \u45768  \u54596 \u49884  \u48708 \u48276 \u54620  \u49324 \u46988 \u51068  \u44163 \u51077 \u45768 \u45796 . \u47560 \u48512  \u51068 \u46020 , \u52380 \u54620  \u51068 \u46020  \u47585 \u44592 \u51648  \u47560 \u49464 \u50836 .\\" \u49849 \u49345 \u51060  \u50739 \u44172  \u50668 \u44200  \u44536  \u47568 \u51012  \u46384 \u46992 \u45796 . \u51060 \u51204 \u50640  \u49849 \u49345 \u51008  \u46041 \u49328 \u50640  \u44867 \u44284  \u45208 \u47924 \u47484  \u47566 \u51060  \u49900 \u50632 \u45716 \u45936 , \u54028 \u44221 \u45432 \u50640 \u44172  \u51060 \u47484  \u44592 \u47476 \u44172  \u54664 \u45796 . \u51060 \u46412 \u48512 \u53552  \u46041 \u49328 \u51032  \u54868 \u52488 \u44032  \u47924 \u49457 \u54616 \u47728  \u51312 \u44552 \u46020  \u49884 \u46308 \u51648  \u50506 \u50500 , \u48393 \u54889 \u51060  \u49933 \u49933 \u51060  \u45216 \u50500 \u46308 \u50612  \u44867 \u44032 \u51648 \u50640  \u44611 \u46308 \u50632 \u45796 . \u50676 \u55128 \u51060  \u51648 \u45228 \u45796 . \u54028 \u44221 \u45432 \u45716  \u49548 \u51200 \u44032  \u46041 \u49328 \u51032  \u44867 \u51012  \u48372 \u44256  \u49910 \u51004 \u45208  \u54028 \u44221 \u45432 \u44032  \u48512 \u45124 \u47084 \u50892  \u50724 \u51648  \u47803 \u54620 \u45796 \u45716  \u47568 \u51012  \u46308 \u50632 \u45796 . \u51060 \u50640  \u54028 \u44221 \u45432 \u45716  \u49849 \u49345 \u51012  \u48473 \u44256  \u47568 \u54664 \u45796 . \\"\u51228 \u44032  \u51060 \u44275 \u50640  \u50728  \u51648  \u50668 \u47084  \u54644  \u51648 \u45228 \u49845 \u45768 \u45796 . \u54620  \u48264 \u46020  \u45432 \u47784 \u47484  \u48473 \u51648  \u47803 \u54664 \u51004 \u45768 , \u45432 \u47784 \u47484  \u48473 \u44256  \u50732  \u47568 \u48120 \u47484  \u51452 \u49901 \u49884 \u50724 .\\" \u49849 \u49345 \u51008  \u45815 \u49352 \u47484  \u51452 \u50632 \u45796 . \u49548 \u51200 \u45716  \u54028 \u44221 \u45432 \u44032  \u44480 \u54693 \u54664 \u45796 \u45716  \u49548 \u49885 \u51012  \u46307 \u44256  \u46041 \u49328 \u50640  \u46308 \u50612 \u50752  \u44867 \u51012  \u48372 \u44256 , \\"\u44867 \u51060  \u45212 \u44036  \u50526 \u50640 \u49436  \u50883 \u45716 \u45936  \u49548 \u47532 \u45716  \u46308 \u47532 \u51648  \u50506 \u45348 .\\"\u46972 \u44256  \u49884 \u47484  \u51648 \u50632 \u45796 . \u54028 \u44221 \u45432 \u45716  \u44867  \u49324 \u51060 \u50640  \u49704 \u50612  \u51080 \u45796 \u44032  \\"\u49352 \u44032  \u49714  \u50500 \u47000 \u49436  \u50864 \u45716 \u45936  \u45576 \u47932  \u48372 \u44592  \u50612 \u47157 \u45348 .\\"\u46972 \u44256  \u49884 \u47196  \u54868 \u45813 \u54664 \u45796 . \u49548 \u51200 \u44032  \u48512 \u45124 \u47084 \u50892  \u50620 \u44404 \u51012  \u48521 \u55176 \u47728  \u46028 \u50500 \u44052 \u45796 . (\u51473 \u47029  \u48512 \u48516  \u51460 \u44144 \u47532 ) \u51473 \u44397  \u54889 \u51228 \u45716  \u49888 \u46972  \u50773 \u50640 \u44172  \u49437 \u54632 \u51012  \u48372 \u45236 , \u44536  \u50504 \u50640  \u51080 \u45716  \u47932 \u44148 \u51012  \u50508 \u50500 \u45236  \u49884 \u47484  \u51648 \u50612  \u50732 \u47532 \u46972  \u47749 \u54620 \u45796 . \u49888 \u46972  \u50773 \u51008  \u51060 \u47484  \u54644 \u44208 \u54616 \u51648  \u47803 \u54616 \u44256  \u45208 \u50629 \u50640 \u44172  \u44284 \u50629 \u51012  \u45336 \u44596 \u45796 . \u45208 \u50629 \u51008  \u51665 \u51004 \u47196  \u46028 \u50500 \u50752  \u49437 \u54632 \u51012  \u50504 \u44256  \u53685 \u44257 \u54664 \u45796 . \u54028 \u44221 \u45432 \u45716  \u51060  \u47568 \u51012  \u46307 \u44256  \u49324 \u46988 \u46308 \u50640 \u44172  \u50780  \u50864 \u45716 \u51648 \u47484  \u47932 \u50632 \u45796 . \u49324 \u46988 \u46308 \u51060  \u47784 \u46160  \u47568 \u54644  \u51452 \u51088 , \u51088 \u47803  \u44592 \u49256 \u51012  \u46944 \u47728  \u44867 \u44032 \u51648 \u47484  \u44734 \u50612  \u50808 \u52397 \u51004 \u47196  \u44052 \u45796 . \u49548 \u51200 \u44032  \u49836 \u54588  \u50872 \u45796 \u44032  \u47928 \u46301  \u48317 \u50640  \u44152 \u47536  \u44144 \u50872 \u50640  \u48708 \u52828  \u44536 \u47548 \u51088 \u47484  \u48372 \u50520 \u45796 . \u49549 \u51004 \u47196  \u45440 \u46972  \u52285 \u53960 \u51004 \u47196  \u50687 \u48372 \u45768  \u54028 \u44221 \u45432 \u44032  \u44867 \u51012  \u46308 \u44256  \u49436  \u51080 \u50632 \u45796 . \u49548 \u51200 \u44032  \u51060 \u49345 \u55176  \u50668 \u44200  \u47931 \u51088 , \u49884 \u52824 \u48120 \u47484  \u46524 \u47728  \u47568 \u54664 \u45796 . \\"\u44536 \u45824 \u44032  \u51060  \u44867 \u51012  \u48372 \u44256  \u49910 \u45796  \u54616 \u50668  \u44536 \u45824 \u47484  \u50948 \u54644  \u44032 \u51256  \u50772 \u49548 . \u49884 \u46308 \u44592  \u51204 \u50640  \u48155 \u50500  \u48372 \u49884 \u50724 .\\" \u49548 \u51200 \u44032  \u54620 \u49704 \u51012  \u53356 \u44172  \u49772 \u45768 , \u54028 \u44221 \u45432 \u44032  \u50948 \u47196 \u54616 \u47728  \u47568 \u54664 \u45796 . \\"\u44144 \u50872  \u49549 \u50640  \u48708 \u52828  \u51060 \u44032  \u48152 \u46300 \u49884  \u44536 \u45824  \u44540 \u49900 \u51012  \u50630 \u50528  \u51460  \u44163 \u51060 \u50724 . \u44540 \u49900 \u52824  \u47568 \u44256  \u44867 \u51012  \u48155 \u51004 \u49884 \u50724 .\\" \u49548 \u51200 \u44032  \u44867 \u51012  \u48155 \u44256  \u48512 \u45124 \u47084 \u50892 \u54616 \u47728  \u50504 \u51004 \u47196  \u46308 \u50612 \u44052 \u45796 . \u50620 \u47560  \u46244  \u49548 \u51200 \u45716  \u54028 \u44221 \u45432 \u51032  \u47568 \u51012  \u44340 \u51060 \u55176  \u50668 \u44200  \u49849 \u49345 \u44760  \u47568 \u54664 \u45796 . \\"\u54028 \u44221 \u45432 \u44032  \u48708 \u47197  \u50612 \u47532 \u51648 \u47564  \u51116 \u51452 \u44032  \u45224 \u48372 \u45796  \u46832 \u50612 \u45208 \u44256 , \u49888 \u51064 (\u31070 \u20154 )\u51032  \u44592 \u50868 \u51060  \u51080 \u50612  \u49437 \u54632  \u49549 \u51032  \u47932 \u44148 \u51012  \u50508 \u50500 \u45236 \u50612  \u49884 \u47484  \u51648 \u51012  \u49688  \u51080 \u51012  \u44163 \u51077 \u45768 \u45796 .\\" \u49849 \u49345 \u51060  \u47568 \u54664 \u45796 . \\"\u45320 \u45716  \u50612 \u52236  \u49789 \u44172  \u47568 \u54616 \u45712 \u45264 ? \u47564 \u50557  \u54028 \u44221 \u45432 \u44032  \u54624  \u49688  \u51080 \u45796 \u47732  \u45208 \u46972 \u51032  \u51060 \u47492 \u45212  \u49440 \u48708  \u44032 \u50868 \u45936  \u54620  \u47749 \u46020  \u49884 \u47484  \u51667 \u51648  \u47803 \u54644  \u51060  \u49437 \u54632 \u51012  \u45208 \u50640 \u44172  \u47585 \u44220 \u44192 \u45712 \u45264 ?\\" \u49548 \u51200 \u44032  \u47568 \u54664 \u45796 . \\"\u48193 \u49352 \u45716  \u48708 \u47197  \u51089 \u51648 \u47564  \u53360  \u49352 \u47588 \u47484  \u49332 \u47536 \u45796  \u54633 \u45768 \u45796 . \u44536 \u44032  \u48708 \u47197  \u45432 \u46164 \u54616 \u45208  \u53360  \u51116 \u51452 \u47484  \u51648 \u45768 \u44256  \u51080 \u45716 \u51648  \u50612 \u52236  \u50508 \u44192 \u49845 \u45768 \u44620 ?\\" \u51060 \u50612 \u49436  \u54028 \u44221 \u45432 \u44032  \u44145 \u51221 \u54616 \u51648  \u47568 \u46972 \u44256  \u54664 \u51020 \u51012  \u44256 \u54664 \u45796 . \\"\u47564 \u50557  \u44536 \u44032  \u49884 \u47484  \u51648 \u51012  \u49688  \u50630 \u45796 \u47732  \u50612 \u52236  \u44536 \u47088  \u47568 \u51012  \u45256 \u44192 \u49845 \u45768 \u44620 ? \u50896 \u52968 \u45824  \u44536 \u47484  \u48520 \u47084  \u49884 \u54744  \u49340 \u50500  \u49884 \u47484  \u51667 \u44172  \u54616 \u49548 \u49436 .\\" \u49849 \u49345 \u51060  \u54028 \u44221 \u45432 \u47484  \u48520 \u47084  \u44396 \u49836 \u47532 \u47728  \u47568 \u54664 \u45796 . \\"\u47564 \u50557  \u51060  \u49437 \u54632  \u49549 \u51032  \u47932 \u44148 \u51012  \u50508 \u50500 \u45236  \u49884 \u47484  \u51667 \u45716 \u45796 \u47732  \u54980 \u54620  \u49345 \u51012  \u51460  \u44163 \u51060 \u47728 , \u47560 \u46405 \u55176  \u45348  \u46907 \u51012  \u51060 \u47336 \u50612  \u51452 \u44192 \u45796 .\\" \u54028 \u44221 \u45432 \u44032  \u44144 \u51208 \u54616 \u47728  \u47568 \u54664 \u45796 . \\"\u51200 \u45716  \u48376 \u47000  \u45796 \u47480  \u45208 \u46972  \u49324 \u46988 \u51077 \u45768 \u45796 . \u48512 \u47784 \u45784 \u44760 \u49436  \u46028 \u50500 \u44032 \u49884 \u51088  \u47676  \u44275 \u50640  \u51109 \u49324  \u51648 \u45236 \u44256  \u49836 \u54548 \u51012  \u51082 \u51004 \u47140 \u44256  \u49888 \u46972 \u50640  \u50772 \u49845 \u45768 \u45796 . \u44536 \u47088 \u45936  \u50612 \u52236  \u54844 \u51064 \u51012  \u54616 \u44192 \u49845 \u45768 \u44620 ?\\" \u49849 \u49345 \u51060  \u46392 \u50640 \u44172  \u47568 \u54664 \u45796 . \\"\u45348 \u44032  \u47564 \u50557  \u51060  \u50500 \u51060 \u51032  \u51116 \u51452 \u47484  \u52845 \u52268 \u54620 \u45796 \u47732  \u47560 \u46405 \u55176  \u45348  \u47568 \u51012  \u46384 \u47476 \u44192 \u45796 .\\" \u49548 \u51200 \u44032  \u47568 \u54664 \u45796 . \\"\u44536 \u44032  \u47564 \u50557  \u49884 \u47484  \u51648 \u51012  \u49688  \u50630 \u45796 \u47732  \u50612 \u52236  \u44048 \u55176  \u44536 \u47088  \u47568 \u51012  \u54664 \u44192 \u49845 \u45768 \u44620 ?\\" \u49849 \u49345 \u51008  \u47560 \u51648 \u47803 \u54644  \u54728 \u46973 \u54616 \u50688 \u45796 . \u54028 \u44221 \u45432 \u45716  \u48531 \u51012  \u45804 \u46972 \u44256  \u54616 \u50668  \u49884 \u47484  \u51648 \u50612  \u50732 \u47160 \u45796 . (\u51473 \u47029 )",\
