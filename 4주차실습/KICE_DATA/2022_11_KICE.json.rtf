    "id": "2022_11_KICE_14-17",\
    "paragraph": "\uc0\u50715 \u45216  \u50612 \u45712  \u47560 \u51012 \u50640  \u54952 \u49457 \u51060  \u51648 \u44537 \u54620  \u45208 \u47924 \u44988 \u51060  \u49332 \u44256  \u51080 \u50632 \u49845 \u45768 \u45796 . \u50612 \u45712  \u45216  \u45208 \u47924 \u44988 \u51008  \u49328 \u50640  \u45208 \u47924 \u47484  \u54616 \u47084  \u44052 \u45796 \u44032  \u45931 \u50640  \u44152 \u47536  \u49324 \u49844 \u51012  \u48156 \u44204 \u54664 \u49845 \u45768 \u45796 . \u45208 \u47924 \u44988 \u51008  \u49324 \u49844 \u51060  \u45931  \u46412 \u47928 \u50640  \u44256 \u53685 \u49828 \u47084 \u50892 \u54616 \u45716  \u47784 \u49845 \u51012  \u48372 \u44256  \u48520 \u49933 \u55176  \u50668 \u44200  \u45931 \u50640 \u49436  \u54400 \u50612  \u51452 \u50632 \u49845 \u45768 \u45796 . \u44536 \u47084 \u51088  \u49324 \u49844 \u51008  \u45208 \u47924 \u44988 \u50640 \u44172  \u44256 \u47577 \u45796 \u45716  \u51064 \u49324 \u47484  \u54616 \u44256  \u49328 \u49549 \u51004 \u47196  \u49324 \u46972 \u51276 \u49845 \u45768 \u45796 . \u45796 \u51020  \u45216 , \u45208 \u47924 \u44988 \u51008  \u49328 \u50640 \u49436  \u45208 \u47924 \u47484  \u54616 \u44256  \u51080 \u45716 \u45936  \u50612 \u51228  \u54400 \u50612  \u51456  \u49324 \u49844 \u51060  \u45208 \u53440 \u45228 \u49845 \u45768 \u45796 . \u49324 \u49844 \u51008  \u45208 \u47924 \u44988 \u51012  \u49714 \u49549  \u44618 \u51008  \u44275 \u51004 \u47196  \u45936 \u47140 \u44052 \u49845 \u45768 \u45796 . \u44536 \u44275 \u50640 \u45716  \u50689 \u54744 \u54620  \u44592 \u50868 \u51060  \u49436 \u47536  \u49888 \u48708 \u54620  \u49368 \u51060  \u51080 \u50632 \u49845 \u45768 \u45796 . \u49324 \u49844 \u51008  \u45208 \u47924 \u44988 \u50640 \u44172  \u49368 \u47932 \u51012  \u47560 \u49884 \u47732  \u45721 \u51648  \u50506 \u44256  \u44148 \u44053 \u54616 \u44172  \u50724 \u47000  \u49332  \u49688  \u51080 \u45796 \u44256  \u50508 \u47140  \u51452 \u50632 \u49845 \u45768 \u45796 . \u45208 \u47924 \u44988 \u51008  \u4